generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GoldTransaction {
  id            Int      @id @default(autoincrement())
  userId        Int
  amountInINR   Float
  goldInGrams   Float
  pricePerGram  Float
  createdAt     DateTime @default(now())
}
